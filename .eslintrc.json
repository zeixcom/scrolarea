{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": [
        "test/**",
        "tests/**",
        "spec/**",
        "**/__tests__/**",
        "test.js",
        "test-*.js",
        "**/*.test.js",
        "**/*.spec.js",
        "**/webpack.config.js",
        "**/webpack.config.*.js",
        "**/*.stories.js"
      ],
      "optionalDependencies": false
    }],
    "no-new": 0,
    "class-methods-use-this": 0,
    "no-trailing-spaces": ["error", { "skipBlankLines": true }]
  },
  "parser": "babel-eslint",
  "globals": {
    "document": true,
    "window": true,
    "Vue": true,
    "_": true,
    "localStorage": true,
    "CustomEvent": true
  }
}
